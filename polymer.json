{
  "entrypoint": "index.html",
  "shell": "src/fh-stundenplan-app/fh-stundenplan-app.html",
  "sources": [
    "src/**/*.{js,html,css,ttf}",
    "icons/*.{png,svg,ico}",
    "index.js"

  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.js",
    "bower_components/webcomponentsjs/*.map",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "safari-js/*"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "preset": "es6-bundled",
      "addPushManifest": true,
      "bundle": {
        "stripComments": true,
        "excludes": [
          "index.html,index.js"
        ]
      }
    }
  ]
}